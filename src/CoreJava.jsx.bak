DatabaseConnection db2 = DatabaseConnection.getInstance();
System.out.println(db1 == db2);  // Output: true

ShapeFactory factory = new ShapeFactory();
Shape shape = factory.getShape("CIRCLE");
shape.draw();  // Output: Drawing Circle`
